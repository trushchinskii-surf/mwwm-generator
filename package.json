{
	"name": "mwwm-generator",
	"displayName": "mwwm-generator",
	"description": "Extension helps you to create mwwm-widgets",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mwwm-generator.create-widget",
				"title": "Create Mwwm-widget"
			},
			{
				"command": "mwwm-generator.create-screen",
				"title": "Create Mwwm-screen"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "mwwm-generator.create-widget",
					"group": "creating",
					"when": "true"
				},
				{
					"command": "mwwm-generator.create-screen",
					"group": "creating",
					"when": "true"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}